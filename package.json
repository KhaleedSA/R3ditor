{
  "main": "App/index.htm",
  "name": "R3ditor",
  "description": "An open-source project to do reverse-engineering some files of Resident Evil 3",
  "js-flags": "--disable-devtools --disable-web-security --allow-file-access-from-files --allow-file-access",
  "version": "0.3.2",
  "keywords": [
    "R3ditor",
    "node-webkit"
  ],
  "window": {
